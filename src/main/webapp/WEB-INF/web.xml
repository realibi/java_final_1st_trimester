    </servlet>
    <servlet-mapping>
        <servlet-name>ServletListOfNews</servlet-name>
        <url-pattern>/ServletListOfNews</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletListOfEvents</servlet-name>
        <servlet-class>ListOfEvents.ServletListOfEvents</servlet-class>
    </servlet>
    <servlet-mapping>
