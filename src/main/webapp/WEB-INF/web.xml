    </servlet>
    <servlet-mapping>
        <servlet-name>ServletChangePassword</servlet-name>
        <url-pattern>/ServletChangePassword</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletCreateRequestClub</servlet-name>
        <servlet-class>CreateRequestClub.ServletCreateRequestClub</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletCreateRequestClub</servlet-name>
        <url-pattern>/ServletCreateRequestClub</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletSelectRequestedClubsParameters</servlet-name>
