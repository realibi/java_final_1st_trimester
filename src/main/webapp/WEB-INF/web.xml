        <servlet-class>LogIn.ServletLogIn</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLogIn</servlet-name>
        <url-pattern>/ServletLogIn</url-pattern>
    </servlet-mapping>

