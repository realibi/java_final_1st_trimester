    </servlet>
    <servlet-mapping>
        <servlet-name>ServletFindStudent</servlet-name>
        <url-pattern>/ServletFindStudent</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletOutPutListOfStudents</servlet-name>
        <servlet-class>FindStudent.ServletOutPutListOfStudents</servlet-class>
